{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "jsx-a11y/alt-text": "off",
    "@next/next/no-img-element": "off",
    "quotes": ["error", "single"]
  }
}
